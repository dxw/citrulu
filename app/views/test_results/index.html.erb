<h1>Listing test_results</h1>

<table>
  <tr>
    <th>Test group</th>
    <th>Assertion</th>
    <th>Value</th>
    <th>Name</th>
    <th>Result</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @test_results.each do |test_result| %>
  <tr>
    <td><%= test_result.test_group_id %></td>
    <td><%= test_result.assertion %></td>
    <td><%= test_result.value %></td>
    <td><%= test_result.name %></td>
    <td><%= test_result.result %></td>
    <td><%= link_to 'Show', test_result %></td>
    <td><%= link_to 'Edit', edit_test_result_path(test_result) %></td>
    <td><%= link_to 'Destroy', test_result, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Test result', new_test_result_path %>
